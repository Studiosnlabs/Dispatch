<merge xmlns:android="http://schemas.android.com/apk/res/android">



    <com.google.android.material.appbar.AppBarLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@drawable/white_gray_border_bottom">

        <androidx.appcompat.widget.Toolbar
            android:layout_width="match_parent"
            android:layout_height="?attr/actionBarSize"
            android:id="@+id/profileToolbarForm">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <!-- <ImageView
                     android:layout_width="25dp"
                     android:layout_height="25dp"
                     android:layout_alignParentLeft="true"
                     android:layout_marginEnd="20dp"
                     android:layout_centerVertical="true"
                     android:id="@+id/profileMenu"
                     android:src="@drawable/ic_baseline_menu_24"/>-->

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:textSize="20sp"
                    android:text="@string/package_details"
                    android:textColor="@color/black"
                    android:layout_toEndOf="@+id/backRegular"
                    android:id="@+id/profileName"/>


                <ImageView
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_centerVertical="true"
                    android:id="@+id/backRegular"
                    android:layout_marginRight="15dp"

                    android:src="@drawable/ic_baseline_back"/>
                <ImageView
                    android:layout_width="27dp"
                    android:layout_height="27dp"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentEnd="true"
                    android:layout_centerVertical="true"
                    android:layout_marginEnd="20dp"
                    android:id="@+id/HelpRegular"
                    android:src="@drawable/ic_baseline_help_24"/>


            </RelativeLayout>




        </androidx.appcompat.widget.Toolbar>



    </com.google.android.material.appbar.AppBarLayout>



</merge>